{
  "form": {
    "div": [
      {
        "@class": "form-row",
        "div": [
          {
            "@class": "form-group col-md-6",
            "label": {
              "@for": "inputEmail4",
              "#text": "Email"
            },
            "input": {
              "@type": "email",
              "@class": "form-control",
              "@id": "inputEmail4",
              "@placeholder": "Email"
            }
          },
          {
            "@class": "form-group col-md-6",
            "label": {
              "@for": "inputPassword4",
              "#text": "Password"
            },
            "input": {
              "@type": "password",
              "@class": "form-control",
              "@id": "inputPassword4",
              "@placeholder": "Password"
            }
          }
        ]
      },
      {
        "@class": "form-group",
        "label": {
          "@for": "inputAddress",
          "#text": "Address"
        },
        "input": {
          "@type": "text",
          "@class": "form-control",
          "@id": "inputAddress",
          "@placeholder": "1234 Main St"
        }
      },
      {
        "@class": "form-group",
        "label": {
          "@for": "inputAddress2",
          "#text": "Address 2"
        },
        "input": {
          "@type": "text",
          "@class": "form-control",
          "@id": "inputAddress2",
          "@placeholder": "Apartment, studio, or floor"
        }
      },
      {
        "@class": "form-row",
        "div": [
          {
            "@class": "form-group col-md-6",
            "label": {
              "@for": "inputCity",
              "#text": "City"
            },
            "input": {
              "@type": "text",
              "@class": "form-control",
              "@id": "inputCity"
            }
          },
          {
            "@class": "form-group col-md-4",
            "label": {
              "@for": "inputState",
              "#text": "State"
            },
            "select": {
              "@id": "inputState",
              "@class": "form-control",
              "option": [
                {
                  "@selected": "",
                  "#text": "Choose..."
                },
                "..."
              ]
            }
          },
          {
            "@class": "form-group col-md-2",
            "label": {
              "@for": "inputZip",
              "#text": "Zip"
            },
            "input": {
              "@type": "text",
              "@class": "form-control",
              "@id": "inputZip"
            }
          }
        ]
      },
      {
        "@class": "form-group",
        "div": {
          "@class": "form-check",
          "input": {
            "@class": "form-check-input",
            "@type": "checkbox",
            "@id": "gridCheck"
          },
          "label": {
            "@class": "form-check-label",
            "@for": "gridCheck",
            "#text": "Check me out"
          }
        }
      }
    ],
    "button": {
      "type": "submit",
      "class": "btn btn-primary",
      "text": "Sign in"
    }
  }
}
